  "description": "http-backend - A simple Express application with TypeScript",
  "main": "index.ts",
  "scripts": {
    "build": "tsc -b",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && npm run start\"",
    "dev:build": "tsc -b --watch"
