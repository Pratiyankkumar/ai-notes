    "mongoose": "^8.0.3",
    "mongodb": "^6.3.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.2"
  },
  "devDependencies": {
    "typescript": "^4.3.5",
    "@types/node": "^15.0.1",
    "@types/express": "^4.17.8",
    "ts-node": "^10.0.0",
    "nodemon": "^2.0.7",
    "@types/mongodb": "^4.0.7",
    "@types/jsonwebtoken": "^9.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
